build_tuple ( #i -- )
x = PyTuple_New(i);
if (x != NULL) {
        for (; --i >= 0;) {
                a1 = POP();
                PyTuple_SET_ITEM(x, i, a1);
        }
        PUSH(x);
        NEXT();
