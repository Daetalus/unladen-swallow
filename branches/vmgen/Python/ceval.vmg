        else if (PyInt_Check(a1)) {
                switch(PyInt_AS_LONG(a1)) {
                case WHY_RETURN:
                case WHY_CONTINUE:
                        /* Retval in TOP. */
                        exit_func = SECOND();
                        SET_SECOND(TOP());
                        SET_TOP(a1);
                        break;
                default:
                        exit_func = TOP();
                        SET_TOP(a1);
                        break;
