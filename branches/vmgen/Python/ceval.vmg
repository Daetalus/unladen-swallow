inplace_subtract ( a1 a2 -- a  dec:a1 dec:a2  next:a )
if (PyInt_CheckExact(a1) && PyInt_CheckExact(a2)) {
        /* INLINE: int - int */
        register long u, v, i;
        u = PyInt_AS_LONG(a1);
        v = PyInt_AS_LONG(a2);
        i = u - v;
        if ((i^u) < 0 && (i^~v) < 0)
