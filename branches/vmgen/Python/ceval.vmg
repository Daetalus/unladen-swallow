        format_exc_check_arg(PyExc_NameError, GLOBAL_NAME_ERROR_MSG, a1);

delete_fast ( #i -- )
x = GETLOCAL(i);
if (x != NULL) {
        SETLOCAL(i, NULL);
        NEXT();
}
format_exc_check_arg(
        PyExc_UnboundLocalError,
        UNBOUNDLOCAL_ERROR_MSG,
        PyTuple_GetItem(co->co_varnames, i));
