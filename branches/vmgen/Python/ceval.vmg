\ _ , UNPACK_SEQUENCE
unpack_sequence ( #i a1 -- )
if (PyTuple_CheckExact(a1) && PyTuple_GET_SIZE(a1) == i) {
        PyObject **items = ((PyTupleObject *)a1)->ob_item;
        while (i--) {
                a2 = items[i];
                Py_INCREF(a2);
                PUSH(a2);
        }
        Py_DECREF(a1);
        NEXT();
} else if (PyList_CheckExact(a1) && PyList_GET_SIZE(a1) == i) {
        PyObject **items = ((PyListObject *)a1)->ob_item;
        while (i--) {
                a2 = items[i];
